    "systemjs-webpack-interop": "^2.3.7",
    "typed-css-modules": "^0.7.2",
    "typescript": "^5.0.4",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.0",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2",
