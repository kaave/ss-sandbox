{
  "name": "@kaave/react-app-common",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:webpack": "webpack serve --env standalone",
    "dev:css-types": "pnpm gen:css-types --watch",
    "gen": "run-p gen:*",
    "gen:css-types": "tcm src",
    "start": "webpack serve --port 7500",
    "build": "pnpm gen; run-p build:* typecheck",
    "build:webpack": "webpack --mode=production",
    "analyze": "webpack --mode=production --env analyze",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:complexity": "eslint src -c .eslintrc.complexity.cjs --ext .ts,.tsx,.js,.jsx",
    "lint:style": "stylelint './src/**/*.css'",
    "lint:spell": "cspell './src/**/*'",
    "fix": "run-p fix:*",
    "fix:scripts": "pnpm prettier; eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "fix:style": "stylelint --fix '**/*.css' && prettier --write '**/*.css'",
    "fix:packagejson": "sort-package-json",
    "prettier": "prettier --write \"src/**/*.+(ts|tsx|json|yml|css|md|mdx)\"",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "test": "vitest",
    "coverage": "jest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "browserslist": [
    "chrome >= 92"
  ],
  "dependencies": {
    "@bufbuild/connect": "^0.8.6",
    "@bufbuild/connect-web": "^0.8.6",
    "@bufbuild/protobuf": "^1.2.0",
    "jotai": "2.0.4",
    "postcss": "^8.4.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "single-spa-react": "^5.0.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.9",
    "@storybook/addon-interactions": "^7.0.9",
    "@storybook/addon-links": "^7.0.9",
    "@storybook/blocks": "^7.0.9",
    "@storybook/react": "^7.0.9",
    "@storybook/react-vite": "^7.0.9",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@swc/core": "^1.3.56",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^14.0.0",
    "@tsconfig/strictest": "^2.0.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@types/systemjs": "^6.1.1",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "babel-plugin-react-css-modules": "^5.2.6",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.5",
    "cspell": "^6.31.1",
    "cssnano": "^6.0.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsdoc": "^43.1.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^1.14.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-testing-library": "^5.10.3",
    "eslint-plugin-unicorn": "^46.0.0",
    "jsdom": "^22.0.0",
    "mini-css-extract-plugin": "^2.7.5",
    "msw": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "postcss-calc": "^9.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.3.0",
    "postcss-url": "^10.1.3",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "single-spa": "^5.9.3",
    "sort-package-json": "^2.4.1",
    "storybook": "^7.0.9",
    "stylelint": "^15.6.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended": "^12.0.0",
